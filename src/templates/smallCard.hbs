{{#each this}}

<li class="smallCard">
    <img class="smallCard-small" src='{{images.[1].url}}' id = '{{id}}' width=200 height=500/>
    <div class="smallCard_info">
        <ul class="smallCard_info_list">
            <li class="smallCard_heroes">
                <p class="smallCard_text">{{name}}</p>
            </li>
            <li class="smallCard_date">
                <p class="smallCard_text">{{dates.start.localDate}}</p>
            </li>
            <li class="smallCard_place">
                <a href="{{url}}" class="smallCard_text">{{_embedded.venues.[0].name}}</a> 
            </li>
        </ul> 
    </div>
</li>

{{/each}}
